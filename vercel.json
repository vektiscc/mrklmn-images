{
  "version": 2,
  "functions": {
    "api/upload.ts": {
      "maxDuration": 10
    },
    "api/delete.ts": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/upload",
      "methods": ["POST"],
      "dest": "api/upload.ts"
    },
    {
      "src": "/api/delete",
      "methods": ["GET"],
      "dest": "api/delete.ts"
    }
  ]
}
